{
  "private": true,
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@tsintergy/role": "^3.4.3",
    "@types/lodash-es": "^4.17.4",
    "@umijs/preset-react": "1.x",
    "antd": "^4.15.1",
    "dva-model-extend": "^0.1.2",
    "echarts": "^5.0.2",
    "echarts-for-react": "^3.0.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "pinyin-match": "^1.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.4.8"
  },
  "devDependencies": {
    "@tsintergy/eslint-config-react": "0.0.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.8",
    "cross-env": "^7.0.3",
    "inquirer": "^8.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
